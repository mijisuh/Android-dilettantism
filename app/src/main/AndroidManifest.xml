<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ddwu.mobile.final_project">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:networkSecurityConfig="@xml/network_security_config">

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google" />

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchCulartAreaActivity"></activity>
        <activity android:name=".SearchCulartRealmActivity"></activity>
        <activity android:name=".CulartDetailActivity"></activity>
        <activity android:name=".SearchPerformingplaceActivity"></activity>
        <activity android:name=".SearchArtgalleryActivity"></activity>
        <activity android:name=".SearchLibraryActivity"></activity>
        <activity android:name=".SearchMuseumActivity"></activity>
        <activity android:name=".CulartPlaceDetailActivity"></activity>

        <activity android:name=".NoteActivity"></activity>
        <activity android:name=".UpdateNoteActivity"></activity>
        <activity android:name=".InsertNoteActivity"></activity>

        <activity android:name=".SearchClassActivity"></activity>
        <activity android:name=".ClassDetailActivity"></activity>

        <activity android:name=".SearchNearbyActivity"></activity>
        <activity android:name=".NearbyDetailActivity"></activity>

        <activity android:name="InspireActivity"></activity>
    </application>



</manifest>